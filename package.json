{
  "name": "plume-ui",
  "description": "plume ui framework",
  "version": "0.1.15",
  "author": "plume-ui team",
  "main": "dist/index.js",
  "style": "dist/index.css",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Japan-Venture-Research/plume-ui",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.3.4",
    "lodash": "^4.17.15",
    "react-tooltip": "^4.2.21"
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "autoprefixer": "^9.6.1",
    "css-loader": "^3.2.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "exec-sh": "^0.3.4",
    "jest": "^25.1.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.1",
    "remark-mdx": "^1.5.1",
    "remark-parse": "^8.0.0",
    "remark-stringify": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.5.3",
    "unified": "^9.0.0",
    "unified-stream": "^1.0.5",
    "unist-util-filter": "^2.0.0",
    "unist-util-find": "^1.0.1",
    "unist-util-map": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "remixicon": "^2.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js --watch",
    "copy:sass": "cd src && find . -name '*.scss' | cpio -pdm ../plume-sass && cd -",
    "clean": "rimraf dist && mkdir -p dist",
    "lint": "prettier --check 'src/**/*{ts,tsx,scss}'",
    "lint:fix": "prettier --write 'src/**/*{ts,tsx,scss}'",
    "test": "jest",
    "test:type": "tsc --noEmit",
    "fmt": "prettier --write '**/*{ts,tsx}'",
    "prepublishOnly": "run-s clean lint build",
    "docs": "docz dev",
    "docs:build": "docz build",
    "docs:typecheck": "node scripts/typecheck.js",
    "hosting-build": "run-s build docs:build",
    "dev": "run-p start docs"
  }
}
